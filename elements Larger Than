function elementsLargerThan(arr, number) {
    let result = [];
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] > number) {
            result.push(arr[i]);
        }
    }
    return result;
}
let numbers = [5, 12, 3, 8, 20, 15];
let threshold = 10;

let largerNumbers = elementsLargerThan(numbers, threshold);
console.log(`Numbers larger than ${threshold}:`, largerNumbers);
