function extractUniqueCharacters(str) {
    let unique = "";
    for (let char of str) {
        if (unique.indexOf(char) === -1) {
            unique += char;
        }
    }
    return unique;
}
let inputString = "thequickbrownfoxjumpsoverthelazydog";
let uniqueChars = extractUniqueCharacters(inputString);
console.log(uniqueChars);
