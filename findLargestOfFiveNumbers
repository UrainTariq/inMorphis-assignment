function findLargestOfFiveNumbers(a, b, c, d, e) {
    let largest = a;
    
    if (b > largest) {
        largest = b;
    }
    if (c > largest) {
        largest = c;
    }
    if (d > largest) {
        largest = d;
    }
    if (e > largest) {
        largest = e;
    }
    
    console.log(`The largest number is: ${largest}`);
}

let num1 = -5, num2 = -2, num3 = -6, num4 = 0, num5 = -1;
findLargestOfFiveNumbers(num1, num2, num3, num4, num5); 
