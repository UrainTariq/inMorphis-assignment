function factorial(n) {
    if (n === 0) {
        return 1; // Base case: 0! is 1
    } else {
        return n * factorial(n - 1); // Recursive case: n! = n * (n-1)!
    }
}

// Example usage:
let number = 5;
console.log(`Factorial of ${number} is: ${factorial(number)}`); // Output: Factorial of 5 is: 120
